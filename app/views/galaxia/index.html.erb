<p id="notice"><%= notice %></p>

<h1 style="color:red">Galaxia</h1>


  <thead>
  <table class="table">
<div class="container"> 
    <tr>
      <th style="color:red">Nombre</th>
      <th style="color:red">Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @galaxia.each do |galaxium| %>
      <tr>
        <td><%= galaxium.nombre %></td>
        <td><%= galaxium.body %></td>
        <td><%= link_to 'Show', galaxium %></td>
        <td><%= link_to 'Edit', edit_galaxium_path(galaxium) %></td>
        <td><%= link_to 'Destroy', galaxium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Galaxium', new_galaxium_path %>


